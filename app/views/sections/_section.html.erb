<h3><%= link_to @section.course.supercourse.title, @section.course.supercourse %></h3>
<%= @section.name %><br>
<% if @section.course.supercourse.title != "Lunch" %>
  <%= link_to @section.course.teacher.full_name, @section.course.teacher %><br>
<% end %>
<%= get_period @section %> period<br>
<%= @students.total_count %> students<br><br>
<table class="table table-striped table-condensed">
  <tr>
    <th>Name</th>
    <th>Year</th>
  </tr>
  <% @students.each do |s| %>
    <tr>
      <td><%= link_to s.full_name, s %></td>
      <td><%= s.grad_year %></td>
    </tr>
  <% end %>
</table>
<%= paginate @students, remote: true %>
